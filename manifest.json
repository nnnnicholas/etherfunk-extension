{
  "manifest_version": 3,
  "name": "View in Etherfunk",
  "version": "1.0",

  "description": "Adds a View in Etherfunk button to contract pages on Etherscan",

  "icons": {
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png",
    "256": "icons/logo-256.png"
  },

  "content_scripts": [
    {
      "matches": [
        "*://etherscan.io/address/*"
      ],
      "js": ["mate.js"]
    }
  ]
}
